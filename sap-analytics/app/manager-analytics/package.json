{
  "name": "ma.manageranalytics",
  "version": "0.0.1",
  "description": "Shows learner&#39;s info test",
  "keywords": [
    "ui5",
    "openui5",
    "sapui5"
  ],
  "main": "webapp/index.html",
  "scripts": {
    "deploy-config_old": "npx -p @sap/ux-ui5-tooling fiori add deploy-config cf",
    "start": "fiori run --open \"test/flpSandbox.html?sap-ui-xx-viewCache=false#mamanageranalytics-tile\"",
    "start-local": "fiori run --config ./ui5-local.yaml --open \"test/flpSandbox.html?sap-ui-xx-viewCache=false#mamanageranalytics-tile\"",
    "start-noflp": "fiori run --open \"index.html?sap-ui-xx-viewCache=false\"",
    "start-variants-management": "fiori run --open \"preview.html?sap-ui-xx-viewCache=false&fiori-tools-rta-mode=true&sap-ui-rta-skip-flex-validation=true#preview-app\"",
    "build": "ui5 build -a --clean-dest --include-task=generateCachebusterInfo",
    "deploy": "fiori verify",
    "deploy-config": "fiori add deploy-config",
    "start-mock": "fiori run --config ./ui5-mock.yaml --open \"test/flpSandbox.html?sap-ui-xx-viewCache=false#mamanageranalytics-tile\"",
    "suite-tests": "fiori run --open 'test/testsuite.qunit.html'",
    "int-tests": "fiori run --open 'test/integration/opaTests.qunit.html'"
  },
  "devDependencies": {
    "@ui5/cli": "^2.14.1",
    "@sap/ux-ui5-tooling": "1",
    "rimraf": "3.0.2",
    "@sap-ux/ui5-middleware-fe-mockserver": "2",
    "@sap/ux-specification": "UI5-1.109"
  },
  "private": true,
  "sapux": true,
  "sapuxLayer": "CUSTOMER_BASE",
  "ui5": {
    "dependencies": [
      "@sap/ux-ui5-tooling",
      "@sap-ux/ui5-middleware-fe-mockserver"
    ]
  }
}